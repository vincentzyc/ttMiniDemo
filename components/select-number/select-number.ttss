.select-number-wrap {
	display: flex;
	padding: 10rpx 20rpx;
}

.numpicker-top {
	height: 40px;
}
.numpicker-input {
	border: 1px solid #ddd;
	border-radius: 20px;
	padding: 0 10px 0 30px;
	margin-right: 10px;
	height: 100%;
}
.numpicker-search-icon {
	position: absolute;
	top: 12px;
	left: 10px;
}
.numpicker-confirm-btn {
	margin-top: 10px;
	border-radius: 40px;
}
.numpicker-change {
	height: 100%;
}
.numpicker-center {
	margin-top: 15px;
}
.numpicker-center .phone-item {
	margin-bottom: 10px;
	width: 45%;
	position: relative;
}
.numpicker-center .phone-number {
	position: relative;
	font-size: 16px;
	border: 2px solid #ebedf0;
	border-radius: 10px;
	line-height: 20px;
	height: 40px;
	background-color: #fff;
}
.numpicker-center .isactive {
	border-color: #f15048
}
.input-wrap .active-char, .numpicker-center .active-char {
	color: #f15048
}
.phone-item.mg-b0 {
	margin-bottom: 0;
}
.is-good-num {
	position: absolute;
	right: -10px;
	top: -10px;
	width: 24px;
	height: 24px;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	background-position: 0 0;
	background-image: url(data:image/png;base64,);
	z-index: 9;
}